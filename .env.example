# Environment variables for GitChangesAnalyzer Docker deployment
# Copy this file to .env and customize for your environment

# Git repository path (absolute path to the repository you want to analyze)
GIT_REPO_PATH=/workspace

# Output directory for analysis reports
OUTPUT_PATH=./output

# Commit hash to analyze (can be overridden in docker-compose command)
COMMIT_HASH=

# Image tag to use
IMAGE_TAG=gitchangesanalyzer:latest

# Docker configuration
COMPOSE_PROJECT_NAME=gitchangesanalyzer

# .NET Environment
DOTNET_ENVIRONMENT=Production

# Analysis configuration
SIMILARITY_THRESHOLD=0.7
INCLUDE_PATTERNS=
EXCLUDE_PATTERNS=
